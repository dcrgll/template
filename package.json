{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 8080",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|gql|graphql|mdx)\"",
    "format:check": "prettier --check \"**/*.+(js|jsx|json|yml|yaml|css|md|gql|graphql|mdx)\"",
    "db:seed": "prisma db seed --preview-feature"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.17.1",
    "@prisma/client": "^4.11.0",
    "autoprefixer": "10.4.14",
    "next": "canary",
    "next-auth": "^4.20.1",
    "postcss": "8.4.21",
    "prisma": "^4.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.7"
  },
  "devDependencies": {
    "@daaaan/eslint-config": "^2.0.3",
    "@daaaan/prettier-config": "^2.0.1"
  },
  "prettier": "@daaaan/prettier-config",
  "eslintConfig": {
    "extends": "@daaaan/eslint-config"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  }
}
